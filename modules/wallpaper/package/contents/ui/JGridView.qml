import QtQuick 2.4
import QtQuick.Layouts 1.1
import QtQuick.Controls 2.3 as Controls

GridView {
    id: root

    cellWidth : width / 6
    cellHeight : cellWidth
    boundsBehavior: Flickable.StopAtBounds
    delegate: GridDelegate {
        id: delegate
        width: root.cellWidth
        height: root.cellHeight
    }

}


